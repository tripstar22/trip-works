{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "camelcase": "warn",
    "max-len": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-one-expression-per-line": [1, { "allow": "single-child" }]
  }
}
